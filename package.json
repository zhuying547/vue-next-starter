{
  "name": "vite-vue-starter",
  "version": "1.8.0",
  "description": "A modern vue admin. It is based on Vue, Vite and TypeScript",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "vite",
    "build": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "eslint --ext .vue,.js,.jsx,.ts,.tsx --ignore-path .gitignore --fix src",
    "lint:fix": "eslint --ext .vue,.js,.jsx,.ts,.tsx src --max-warnings 0 --fix",
    "format": "prettier --write --cache .",
    "commit": "git-cz",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [],
  "author": "zhuying547 <zhuying547@qq.com> (https://github.com/zhuying547)",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@commitlint/cz-commitlint": "^17.5.0",
    "@tsconfig/node18": "^18.2.0",
    "@types/node": "^18.17.5",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.14",
    "commitizen": "^4.3.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-vue": "^9.14.1",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tailwindcss": "^3.3.2",
    "typescript": "~5.1.6",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.9",
    "vitepress": "1.0.0-beta.5",
    "vue-tsc": "^1.8.8"
  },
  "dependencies": {
    "tdesign-vue-next": "^1.5.4",
    "tippy.js": "^6.3.7",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "prettier --cache --write",
      "eslint --cache --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
